#!/usr/bin/env nujel

[def run-count 10,000,000]

[defun test-walker [runs]
       [def ret 0]
       [for [i 0 runs]
            [set! ret [+ ret i]]]
       ret]

[defnd test-bytecode [runs]
       [def ret 0]
       [for [i 0 runs]
            [set! ret [+ ret i]]]
       ret]

#;[pfmtln "The result is: {}" [test-walker run-count]]
[pfmtln "The bytecode result is: {}" [test-bytecode run-count]]
