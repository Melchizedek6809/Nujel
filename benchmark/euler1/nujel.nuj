#!/usr/bin/env nujel
;; Multiples of 3 or 5 below 10,000,000
;; https://projecteuler.net/problem=1

[defn test-run []
      [def ret 0]
      [for [i 0 10,000,000]
        [when [or [zero? [% i 3]]
                  [zero? [% i 5]]]
          [set! ret [+ ret i]]]]
      [return ret]]

[pfmtln "The sum is: {}" [test-run]]
