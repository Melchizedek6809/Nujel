("nuj" (path/extension "test.nuj"))
("nuj" (path/extension "Another/test.nuj"))
("NUJ" (upper-case (path/extension "Another/test.nuj")))
("no" (path/extension "asd/test.nuj.no"))
("asd/test.nuj" (path/without-extension "asd/test.nuj.no"))
("" (path/dirname "test.nuj"))
